      "source": [
        "def count_digit_one(n):\n",
        "    count = 0\n",
        "    for i in range(n + 1):\n",
        "        count += str(i).count('1')\n",
        "    return count\n",
        "\n",
        "n = 0\n",
        "output = count_digit_one(n)\n",
        "print(output)\n"
      ]
    }
